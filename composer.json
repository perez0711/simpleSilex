{
    "name": "simple/api",
    "description": "SimpleApi",
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.5",
        "silex/silex": "v1.3.4",
        "twig/twig": "~1.20|~2.0",
        "symfony/twig-bridge": "v2.7.5",
        "monolog/monolog": "^1.17",
        "doctrine/dbal": "^2.5",
        "deralex/yaml-config-service-provider": "^1.0",
        "marcojanssen/silex-routing-service-provider": "^1.3",
        "dflydev/doctrine-orm-service-provider": "1.0.*@dev",
        "doctrine/migrations": "^1.1",
        "symfony/translation": "~2.0",
        "respect/validation": "^1.0",
        "symfony/yaml": "~2.4",
        "symfony/console": "2.5",
        "jms/serializer": "^1.1",
        "lcobucci/jwt": "^3.1",
        "fiunchinho/rabbitmq-service-provider": "dev-master",
        "guzzlehttp/guzzle": "^6.2",
        "bshaffer/oauth2-server-php":"~1.8",
        "bshaffer/oauth2-server-httpfoundation-bridge": "v1.0",
        "elasticsearch/elasticsearch": "^2.2",
        "beberlei/DoctrineExtensions": "^1.0",
        "leafo/scssphp": "0.6.7",
        "bluerhinos/phpmqtt": "dev-master",
        "doctrine/mongodb-odm": "^1.0",
        "neutron/mongo-odm-silex-provider": "*",
        "moriony/silex-mongo-provider": "*"
    },
    "require-dev": {
        "symfony/browser-kit": "*"
    },
    "autoload": {
        "psr-0": {
            "SimpleApi\\": "src/"
        }
    },
    "config": {
        "process-timeout": 9000,
        "secure-http":false
    }
}
